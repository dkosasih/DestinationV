{
  "name": "destinationv-ui",
  "version": "1.0.0",
  "description": "FrontEnd for DestinationV API",
  "keywords": [
    "Angular"
  ],
  "author": "Deddy Kosasih",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "tdd": "jest --watchAll",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.config.ts",
    "roots": [
      "<rootDir>/src/app"
    ],
    "rootDir": ".",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|angular2-ui-switch|ng-dynamic)"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "~7.1.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "~7.1.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ngrx/effects": "~6.1.2",
    "@ngrx/store": "~6.1.2",
    "@ngrx/store-devtools": "~6.1.2",
    "bootstrap": "4.3.1",
    "core-js": "~2.5.4",
    "hammerjs": "~2.0.8",
    "luxon": "~1.5.0",
    "moment": "~2.29.4",
    "ngx-material-timepicker": "~2.11.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.2",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@ngrx/schematics": "~6.1.2",
    "@types/jest": "~23.3.12",
    "@types/luxon": "~1.4.1",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jest": "~23.6.0",
    "jest-preset-angular": "~6.0.2",
    "ngrx-store-freeze": "~0.2.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
